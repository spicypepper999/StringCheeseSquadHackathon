<html>
<head>
    <link rel="stylesheet" href="/stylesheets/styles.css">
    <title>User</title>
</head>
<body>
    <%- include('partials/userbar') %>
    <h1><%= user.username %></h1>
    <% for(let entry of entries){ %>
        <div class="container"><img src="\assets\thought.png" alt="" style="width:75%">
            <div class="bubbletext">
        <h2><%= entry.name %></h2>
        <p><%= entry.date %></p>
        <p><%= entry.text %></p>
        <b><p><%= entry.username %></p></b>
        <% for(let emotion of entry.emotions){ %>
        <i style="background-color:<%=emotion.color%>;"><%= emotion.name %> </i>
        <% } %>
            </div>
        <br>
        <br>
        </div>
    <%} %>
</body>
</html>