<html>
<head>
    <link rel="stylesheet" href="/stylesheets/styles.css">
    <title>User</title>
</head>
<body>
    <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/entry">New Entry</a></li>
        <li><a href="/user">My Entries</a></li>
        <li><a href="/about">About</a></li>
        <li><a href="/register">Register</a></li>
        <li id="user"><a href="/user"><img src="\assets\defaultpfp.jpg" alt=""></a></li>
      </ul>
    <h1><%= user.username %></h1>
    
    <% for(let entry of entries){ %>
        <div class="container"><img src="\assets\thought.png" alt="">
            <div class="bubbletext">
        <h2><%= entry.name %></h2>
        <p><%= entry.text %></p>
        <b><p><%= entry.username %></p></b>
        <% for(let emotion of entry.emotions){ %>
        <i style="background-color:<%=emotion.color%>;"><%= emotion.name %> </i>
        <%} %>
        </div>
        <br>
        <br>
    </div>
    <%} %>
</body>
</html>